<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Logowanie</servlet-name>
        <servlet-class>Logowanie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rejestracja</servlet-name>
        <servlet-class>Rejestracja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Danelogowania</servlet-name>
        <servlet-class>Danelogowania</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Danerejestracji</servlet-name>
        <servlet-class>Danerejestracji</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logowanie</servlet-name>
        <url-pattern>/Logowanie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rejestracja</servlet-name>
        <url-pattern>/Rejestracja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Danelogowania</servlet-name>
        <url-pattern>/Danelogowania</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Danerejestracji</servlet-name>
        <url-pattern>/Danerejestracji</url-pattern>
    </servlet-mapping>
</web-app>